volume = {14},
year = {2022}
}
@article{Vrenken2016,
abstract = {Transient receptor potential (TRP) channels comprise a family of cation channels implicated in a variety of cellular processes, including proliferation, cell migration and cell survival. As a consequence, members of this ion family play prominent roles during embryonic development, tissue maintenance and cancer progression. Although most TRP channels are non-selective, many cellular responses, mediated by TRP channels, appear to be calcium-dependent. In addition, there is mounting evidence for channel-independent roles for TRP channels. In this review, we will discuss how both these channel-dependent and -independent mechanisms affect cellular programs essential during embryonic development, and how perturbations in these pathways contribute to a variety of pathologies. This article is part of a Special Issue entitled: Calcium and Cell Fate. Guest Editors: Jacques Haiech, Claus Heizmann, Joachim Krebs, Thierry Capiod and Olivier Mignen.},

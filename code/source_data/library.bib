file = {:C\:/Users/giorg/Documents/P{\'{e}}rez-Torras et al. - 2016 - Transportome Profiling Identifies Profound Alterations in Crohn's Disease Partially Restored by Commensal B.pdf:pdf},
issn = {1876-4479},
journal = {Journal of Crohn's & colitis},

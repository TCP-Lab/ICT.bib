journal = {Cellular physiology and biochemistry : international journal of experimental cellular physiology, biochemistry, and pharmacology},
keywords = {Animals,Caixia Liu,Carrier Proteins / genetics,Carrier Proteins / metabolism,Databases,Diet,Down-Regulation,Fetal Growth Retardation / etiology,Fetal Growth Retardation / metabolism,Histone-Lysine N-Methyltransferase / genetics,Histone-Lysine N-Methyltransferase / metabolism,Histones / genetics,Histones / metabolism,Jun Wang,Kidney / metabolism*,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Newborn,Oxidative Phosphorylation,Protein,Protein-Restricted*,Proteome / analysis*,Proteomics*,PubMed Abstract,Purines / metabolism,Pyrimidines / metabolism,Rats,TRPP Cation Channels / genetics,TRPP Cation Channels / metabolism,Up-Regulation,Wistar,Xiaomei Liu,doi:10.1159/000484626,pmid:29130966},

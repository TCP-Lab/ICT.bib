keywords = {Ca2+ channels,Cancer,K+ channels,LL-37,Lipids,SIGMAR1,STIM},
pages = {157--176},
pmid = {32767122},
title = {{Potassium and Calcium Channel Complexes as Novel Targets for Cancer Research}},
volume = {183},
year = {2022}
}
@article{Fan2022,
abstract = {Ion channels are pore-forming transmembrane proteins that govern ion flux to regulate a myriad of biological processes in development, physiology, and disease. Across various types of cancer, ion channel expression and activity are often dysregulated. We review the contribution of ion channels to multiple stages of tumorigenesis based on data from in vivo model systems. As intertumoral and intratumoral heterogeneities are major obstacles in developing effective therapies, we provide perspectives on how ion channels in tumor cells and their microenvironment represent targetable vulnerabilities in the areas of tumor-stromal cell interactions, cancer neuroscience, and cancer mechanobiology.},
author = {Fan, Jerry J. and Huang, Xi},
